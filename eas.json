{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "developmentClient": true
    },
    "release": {
      "distribution": "internal",
      "developmentClient": false,
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production-ios": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "store",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      }
    },
    "production-android-aab": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "production-android-apk": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "store",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {}
}
